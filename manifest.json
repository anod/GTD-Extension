{
  "background": {
    "persistent": false,
    "page": "views/background.html"
  },
  "browser_action": {
    "default_icon": "graphics/icon_128.png"
  },
  "default_locale": "en",
  "description": "__MSG_gmailcheck_description__",
  "icons": {
    "128": "graphics/icon_128.png"
  },
  "name": "__MSG_gmailcheck_name__",
  "options_page": "views/options.html",
  "permissions": [
    "alarms",
    "tabs",
    "notifications",
    "webNavigation",
    "*://*.google.com/",
